{
    "baseURL": "https://rewards.bing.com",
    "sessionPath": "sessions",
    "headless": false,
    "runOnZeroPoints": false,
    "clusters": 1,
    "saveFingerprint": false,
    "workers": {
        "doDailySet": true,
        "doMorePromotions": true,
        "doPunchCards": true,
        "doDesktopSearch": true,
        "doMobileSearch": true,
        "doDailyCheckIn": true,
        "doReadToEarn": true
    },
    "globalTimeout": 30000,
    "searchSettings": {
        "useGeoLocaleQueries": true,
        "scrollRandomResults": false,
        "clickRandomResults": false,
        "searchDelay": {
            "min": 8000,
            "max": 10000
        },
        "retryMobileSearch": true
    },
    "webhook": {
        "enabled": false,
        "url": ""
    },
    "cronStartTime": "0 5,11 * * *"
}
